<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_emprendimientos"
        tools:context=".EmprendimientosActivity">
    <android.support.v7.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="8dp" android:id="@+id/RecyclerView_Emprendimientos_Usuario"
            android:layout_marginBottom="50dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"/>
    <android.support.design.widget.FloatingActionButton
            android:id="@+id/fab3"
            android:layout_width="56dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end|bottom"
            android:src="@drawable/ic_add_black_24dp"
            android:layout_margin="16dp" android:layout_marginEnd="37dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:backgroundTint="#FFA347" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="@+id/RecyclerView_Emprendimientos_Usuario"/>
    <Button
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:id="@+id/button7" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/RecyclerView_Emprendimientos_Usuario"
            android:background="@drawable/boton_home" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="9dp"
            app:layout_constraintVertical_bias="0.2" android:layout_marginStart="36dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:id="@+id/button8"
            android:background="@drawable/boton_search"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/RecyclerView_Emprendimientos_Usuario"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintStart_toEndOf="@+id/button7" android:layout_marginStart="47dp"/>
    <Button
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:id="@+id/boton_ticket3"
            android:background="@drawable/boton_ticket"
            app:layout_constraintEnd_toStartOf="@+id/button9"
            android:layout_marginEnd="47dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="9dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/RecyclerView_Emprendimientos_Usuario"/>
    <Button
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:id="@+id/button9"
            android:background="@drawable/boton_alerta" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="45dp"
            android:layout_marginBottom="9dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/RecyclerView_Emprendimientos_Usuario" android:layout_marginTop="8dp"
    />
    <ImageButton
            android:layout_width="40dp"
            android:layout_height="40dp" app:srcCompat="@drawable/tasa_cafe" android:id="@+id/crear_evento3"
            android:background="@drawable/boton_redondo"
            android:visibility="gone" android:contentDescription="@string/imagen_para_crear_evento"
            app:layout_constraintStart_toStartOf="@+id/fab3" app:layout_constraintEnd_toEndOf="@+id/fab3"
            android:layout_marginBottom="10dp" app:layout_constraintBottom_toTopOf="@+id/fab3"/>
    <ImageButton
            android:layout_width="42dp"
            android:layout_height="40dp" app:srcCompat="@drawable/estrella_mitad"
            android:id="@+id/crear_emprendimiento3"
            android:background="@drawable/boton_redondo"
            android:visibility="gone" android:contentDescription="@string/boton_para_crear_emprendimiento"
            app:layout_constraintStart_toStartOf="@+id/fab3" app:layout_constraintEnd_toEndOf="@+id/fab3"
            android:layout_marginBottom="58dp" app:layout_constraintBottom_toTopOf="@+id/fab3"/>
</android.support.constraint.ConstraintLayout>